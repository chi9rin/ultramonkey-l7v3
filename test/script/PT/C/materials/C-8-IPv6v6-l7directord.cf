virtual=[::1]:50000
    real=[::1]:50001 masq
    real=[::1]:50002 masq
    module=sessionless
    scheduler=rr
    protocol=tcp
    checktype=custom
    customcheck=wget -q -O /dev/null http://[::1]:50001/index.html
    customcheck=wget -q -O /dev/null http://[::1]:50002/index.html
    request="/index.html"
    receive="Real"
    service=http

