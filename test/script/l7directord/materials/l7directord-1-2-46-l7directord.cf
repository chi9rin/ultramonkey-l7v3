checktimeout=1
negotiatetimeout='1'
checkinterval="1"
retryinterval    = 1
configinterval   = '1'
checkcount            =  "1"

autoreload   = yes
quiescent    = "no"

fallback="1.1.1.1:80"

callback  =  /bin/sh

execute=/etc/hosts
execute='/etc/passwd'
execute="/etc/group"

supervised

logfile     =   "local0"


virtual=1.1.1.1:80
    real=localhost
    real=1.1.1.1
    request=foobar
    receive=foobar
    login=foobar
    passwd=foobar
    database=foobar
    virtualhost=foobar
    checktype = connect
    fallback="localhost:http"
    module    =  'ip'
    sorryserver="localhost:80"

virtual='2.2.2.2:http'
    real    =   '127.0.0.1->127.0.0.2:http'
    real    =   '127.0.0.3:http masq'
    real    =   '127.0.0.4:http 10 req, rec'
    request     ='foobar'
    receive     ='foobar'
    login       ='foobar'
    passwd      ='foobar'
    database    ='foobar'
    virtualhost ='foobar'
    checktype   =negotiate
    checkport   =   "1"
    scheduler   =   'lc'
    service='https'
    httpmethod    =  HEAD
    fallback = 1.1.1.1:80
    quiescent    = "yes"
    module    =  'sessionless --other option'
    sorryserver    =       localhost:http
    qosup    =  '1G'
    qosdown    =  1M

virtual    =    "localhost:80"
    real   =   localhost:80 masq 10
    real   =   "localhost:25 10"
    request="foobar"
    receive="foobar"
    login  ="foobar"
    passwd ="foobar"
    database   ="foobar"
    virtualhost="foobar"
    checktype  =ping
    checktimeout    =   "1"
    negotiatetimeout=   "1"
    checkinterval   =   "1"
    retryinterval   =   "1"
    checkcount      =   "1"
    maxconn         =   "1"
    scheduler   =   "wrr"
    protocol = 'tcp'
    quiescent    = "no"
    module    =  sslid --other option
    sorryserver  =   "2.2.2.2:http"
    qosup="1M"
    qosdown    =  1G
    realrecovercallback="/bin/bash"
    realdowncallback    =  /bin/sh

virtual    =    localhost:http
    real="localhost->localhost:80 masq 100 request receive"
    real   =    "1.1.1.1:80 masq 100 request 'receive'"
    real   =    "2.2.2.2->2.2.2.5 masq 'request' receive"
    login       =   foobar
    passwd      =   foobar
    database    =   foobar
    virtualhost =   foobar
    request     =   'foobar'
    receive     =   'foobar'
    checktype=10
    checktimeout=1
    negotiatetimeout=1
    checkinterval=1
    retryinterval=1
    checkcount=1
    maxconn=1
    checkport   =   65535
    scheduler   =   rr
    protocol="tcp"
    service="sip"
    fallback   =     'localhost:80'
    quiescent=yes
    module="url --other option --pattern-match 'foo'"
    qosup='999K'
    qosdown    =  999K
    realdowncallback="/bin/bash"
    realrecovercallback=/bin/sh

virtual  = [::1]:88
        real = [2002::230]:80
        real = [2002::231]:80
        module      = sessionless
        scheduler   = rr
        sorryserver = [2002::232]:80
        maxconn     = 1000
        qosup       = 100M
        qosdown     = 100M
        checktype   = negotiate
        service     = http
        request     = "index.html"
        receive     = "html"
        quiescent   = no
        checkcount  = 2
        #realdowncallback        = /opt/down.sh
        #realrecovercallback = /opt/recover.sh
        #customcheck = ping -c1 -w1 _IP_
        # sslconfigfile=SSL_CONFIGFILE_PATH
        # sslconfigfile=/tmp/l7directord/ssl.cf
        # socketoption=OPTION,OPTION,OPTION
        socketoption=deferaccept,nodelay,cork,quickackon
        # accesslog=yes
        accesslog=yes
        # accesslogfile=ACCESSLOG_FILE_PATH
        accesslogfile=/tmp/l7directord/access2.log
        # accesslog_rotate_type=datesize
        accesslog_rotate_type=size
        # accesslog_rotate_max_backup_index=2
        accesslog_rotate_max_backup_index=2
        accesslog_rotate_max_filesize =100M
        accesslog_rotate_rotation_timing=month
        accesslog_rotate_rotation_timing_value="1 23:59"
